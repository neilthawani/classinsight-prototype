{
  "name": "classinsight",
  "version": "1.0.0",
  "description": "ClassInSight: Insight on Teacher Learning by Scaffolding Noticing and Reflection",
  "main": "src/server/server.js",
  "scripts": {
    "app_install": "rm -rf node_modules && npm install",
    "build": "webpack --mode production",
    "start": "npm run build && node src/server/server.js",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "nodemon src/server/server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "kill-ports": "npx kill-port 3000 8802 27017 && killall node",
    "migrate:up": "migrate-mongo up",
    "migrate:down": "migrate-mongo down"
  },
  "author": "Neil Thawani",
  "dependencies": {
    "@babel/preset-typescript": "^7.12.17",
    "@mdi/font": "^5.8.55",
    "@mdi/js": "^5.8.55",
    "@mdi/react": "^1.4.0",
    "axios": "^0.21.1",
    "babel-polyfill": "^6.26.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "classnames": "^2.2.6",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "d3": "^6.3.1",
    "dotenv-expand": "5.1.0",
    "express": "^4.16.3",
    "file-system": "^2.2.2",
    "html-webpack-plugin": "^3.2.0",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "mongodb": "^3.5.9",
    "mongoose-sequence": "^5.3.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-google-login": "^5.2.2",
    "react-papaparse": "^3.11.1",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "sass": "^1.30.0",
    "tunnel-ssh": "^4.1.4",
    "validator": "^13.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "babel-eslint": "^10.0.0",
    "babel-loader": "^8.0.0",
    "clean-webpack-plugin": "^1.0.0",
    "concurrently": "^4.0.0",
    "dotenv": "^8.2.0",
    "file-loader": "^3.0.0",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^1.17.3",
    "style-loader": "^0.23.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.5.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.11.0"
  }
}
