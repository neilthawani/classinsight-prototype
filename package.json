{
  "name": "classinsight-server",
  "repository": "http://www.github.com/neilthawani/classinsight-prototype",
  "version": "1.0.0",
  "description": "ClassInSight Backend",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "app_install": "rm -rf node_modules && rm -rf client/node_modules && npm install && npm run client-install",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "kill-ports": "npx kill-port 3000 8802 27017 && killall node",
    "migrate:up": "migrate-mongo up",
    "migrate:down": "migrate-mongo down"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "aws4": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "child_process": "^1.0.2",
    "concurrently": "^5.3.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "defaultable": "^0.7.2",
    "dgram": "^1.0.1",
    "dns": "^0.2.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "file-system": "^2.2.2",
    "fs": "0.0.1-security",
    "http-proxy": "^1.18.1",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "module": "^1.2.5",
    "mongodb-client-encryption": "^1.2.1",
    "mongoose": "^5.11.8",
    "mongoose-sequence": "^5.3.0",
    "net": "^1.0.2",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "require_optional": "^1.0.1",
    "tls": "0.0.1",
    "tunnel-ssh": "^4.1.4",
    "validator": "^13.5.1",
    "webpack": "^5.21.2",
    "webpack-cli": "^4.5.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.6"
  },
  "browser": {
    "fs": false,
    "child_process": false,
    "dgram": false
  }
}
